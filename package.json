{
  "name": "snapshot-testing-sample",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/kentt8046/snapshot-testing-sample.git",
  "author": "kentt <kentt8046@gmail.com>",
  "scripts": {
    "test": "jest ./src"
  },
  "dependencies": {
    "react": "16.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.0",
    "@types/jest": "21.1.3",
    "@types/node": "8.0.44",
    "@types/react": "16.0.13",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.2",
    "jest": "21.2.1",
    "jest-enzyme": "4.0.1",
    "jest-react-vrt": "file:../jest-react-vrt",
    "ts-jest": "21.1.3",
    "typescript": "2.5.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "transform": {
      "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
